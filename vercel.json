{
    "headers": [
      {
        "source": "/icons/(.*.svg)", 
        "headers": [
          {
            "key": "Content-Type",
            "value": "image/svg+xml" 
          },
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ],
    "routes": [
      {
        "src": "/_nuxt/(.*)",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable"
        }
      }
    ]
  }